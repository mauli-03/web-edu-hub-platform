<svg xmlns="http://www.w3.org/2000/svg" width="400" height="240" viewBox="0 0 400 240">
  <!-- Background - Sky -->
  <rect width="400" height="240" fill="#e0f2fe" />
  
  <!-- Ground -->
  <rect y="180" width="400" height="60" fill="#4ade80" />
  
  <!-- Path -->
  <path d="M50,180 C100,160 300,200 350,180 L350,240 L50,240 Z" fill="#d6d3d1" />
  
  <!-- Buildings -->
  <!-- Main Building -->
  <g transform="translate(150, 70)">
    <rect x="0" y="40" width="100" height="70" fill="#4f46e5" />
    <polygon points="0,40 50,10 100,40" fill="#3730a3" />
    <rect x="40" y="80" width="20" height="30" fill="#1f2937" />
    <rect x="10" y="50" width="15" height="20" fill="#e5e7eb" />
    <rect x="35" y="50" width="15" height="20" fill="#e5e7eb" />
    <rect x="60" y="50" width="15" height="20" fill="#e5e7eb" />
    <rect x="85" y="50" width="15" height="20" fill="#e5e7eb" />
  </g>
  
  <!-- Library Building -->
  <g transform="translate(50, 100)">
    <rect x="0" y="30" width="70" height="50" fill="#8b5cf6" />
    <rect x="25" y="60" width="20" height="20" fill="#1f2937" />
    <rect x="10" y="40" width="15" height="15" fill="#e5e7eb" />
    <rect x="45" y="40" width="15" height="15" fill="#e5e7eb" />
  </g>
  
  <!-- Science Building -->
  <g transform="translate(280, 110)">
    <rect x="0" y="20" width="60" height="50" fill="#6366f1" />
    <rect x="20" y="50" width="20" height="20" fill="#1f2937" />
    <rect x="10" y="30" width="10" height="10" fill="#e5e7eb" />
    <rect x="30" y="30" width="10" height="10" fill="#e5e7eb" />
    <rect x="50" y="30" width="10" height="10" fill="#e5e7eb" />
  </g>
  
  <!-- Trees -->
  <g transform="translate(30, 150)">
    <circle cx="0" cy="0" r="15" fill="#16a34a" />
    <rect x="-2" y="0" width="4" height="30" fill="#a16207" />
  </g>
  
  <g transform="translate(80, 160)">
    <circle cx="0" cy="0" r="12" fill="#16a34a" />
    <rect x="-2" y="0" width="4" height="20" fill="#a16207" />
  </g>
  
  <g transform="translate(320, 150)">
    <circle cx="0" cy="0" r="15" fill="#16a34a" />
    <rect x="-2" y="0" width="4" height="30" fill="#a16207" />
  </g>
  
  <g transform="translate(370, 165)">
    <circle cx="0" cy="0" r="10" fill="#16a34a" />
    <rect x="-2" y="0" width="4" height="15" fill="#a16207" />
  </g>
  
  <!-- Sun -->
  <circle cx="50" cy="50" r="20" fill="#fbbf24" />
  <circle cx="50" cy="50" r="25" fill="#fbbf24" opacity="0.3" />
  
  <!-- Text -->
  <text x="200" y="220" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#1f2937">Campus View</text>
</svg>